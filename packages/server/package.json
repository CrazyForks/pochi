{
  "name": "@ragdoll/server",
  "main": "src/index.ts",
  "dependencies": {
    "@ai-sdk/google": "^1.2.3",
    "@ai-sdk/openai": "^1.3.2",
    "@better-auth/stripe": "^1.2.5",
    "@hono/zod-validator": "^0.4.3",
    "@neondatabase/serverless": "^1.0.0",
    "@openrouter/ai-sdk-provider": "^0.4.4",
    "@ragdoll/tools": "workspace:*",
    "ai": "^4.2.0",
    "better-auth": "^1.2.5",
    "hono": "^4.7.5",
    "kysely": "^0.27.6",
    "kysely-neon": "^1.3.0",
    "stripe": "^18.0.0",
    "ws": "^8.18.1",
    "zod": "^3.24.2"
  },
  "scripts": {
    "tsc": "tsc --noEmit",
    "dev:server": "bun --hot src/server.ts",
    "build": "bun build src/server.ts --outdir dist --target bun",
    "start": "bun run dist/server.js",
    "db:migrate": "bunx @better-auth/cli migrate -y && bun run scripts/migrate-db-schema.ts",
    "db:genschema": "kysely-codegen --out-file ./src/db/schema.d.ts"
  },
  "devDependencies": {
    "kysely-codegen": "^0.18.3"
  }
}
