    "@hono/zod-validator": "^0.4.3",
    "@instantdb/admin": "^0.17.32",
    "@openrouter/ai-sdk-provider": "^0.4.4",
    "@ragdoll/tools": "workspace:*",
    "ai": "^4.2.0",
    "hono": "^4.7.5",
    "zod": "^3.24.2"
  }
}
