{
  "name": "@ragdoll/server",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./instant.schema": "./instant.schema.ts"
  },
  "scripts": {
    "tsc": "tsc --noEmit",
